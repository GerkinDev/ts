{
  "name": "@mmkal/js",
  "version": "0.0.1",
  "description": "js monorepo",
  "main": "index.js",
  "repository": "https://github.com/mmkal/js",
  "author": "mmkal <misha.kaletsky@gmail.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "echo-commit-tag": "echo \"$(git log -n 1 --date=format:'v%Y.%m.%d.%H.%M.%S' --pretty=format:'%ad__%H')\" | cut -c1-29",
    "prepatch-version": "lerna version prepatch --preid $(yarn --silent echo-commit-tag) --no-git-tag-version --no-push --no-changelog",
    "publish-packages": "lerna publish from-package --registry https://npm.pkg.github.com"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.19",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "js-yaml": "^3.13.1",
    "lerna": "^3.18.1",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "typescript": "^3.7.4"
  }
}
